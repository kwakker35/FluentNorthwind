{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (TestHarness.Console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build TestHarness.Console",
      "program": "${workspaceFolder}/src/TestHarness.Console/bin/Debug/net8.0/TestHarness.Console.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/TestHarness.Console",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
