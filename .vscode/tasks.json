{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build FluentNorthwind.Lib",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/FluentNorthwind.Lib/FluentNorthwind.Lib.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build TestHarness.Console",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/TestHarness.Console/TestHarness.Console.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "dependsOn": "build FluentNorthwind.Lib"
    }
  ]
}
